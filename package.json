{
	"name": "tyk-schemas",
	"repository": {
		"type": "git",
		"url": "https://github.com/TykTechnologies/vscode-tyk"
	},
	"publisher": "TykTechnologiesLimited",
	"icon": "images/Tyk-logo.png",
	"license": "MPL-2.0",
	"displayName": "Tyk Schemas",
	"description": "Tyk Schemas provides schemas for your Tyk related configuration files.",
	"version": "0.0.5",
	"homepage": "https://github.com/TykTechnologies/vscode-tyk/blob/main/README.md",
	"bugs": {
		"url": "https://github.com/TykTechnologies/vscode-tyk/issues"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"keywords": [
		"Tyk",
		"Tyk config",
		"API Gateway",
		"JSON validation",
		"JSON"
	],
	"activationEvents": [
		"onDebug",
		"onStartupFinished",
		"onCommand:tyk-schemas.main"
	],
	"main": "./src/extension.js",
	"contributes": {
		"jsonValidation": [
			{
				"fileMatch": [
					"tyk.*.conf"
				],
				"url": "https://raw.githubusercontent.com/TykTechnologies/tyk-schemas/main/JSON/draft-07/schema_tyk.oss.conf"
			},
			{
				"fileMatch": [
					"apikey.*.json"
				],
				"url": "https://raw.githubusercontent.com/TykTechnologies/tyk-schemas/main/JSON/draft-07/schema_apikey.json"
			},
			{
				"fileMatch": [
					"apidef.*.json",
					"TykDefinition-*.json"
				],
				"url": "https://raw.githubusercontent.com/TykTechnologies/tyk-schemas/main/JSON/draft-07/schema_apidef_lean.json"
			},
			{
				"fileMatch": [
					"oasapidef.*.json",
					"TykOasApiDef-*.json"
				],
				"url": "https://raw.githubusercontent.com/TykTechnologies/tyk-schemas/main/JSON/draft-04/schema_apidefoas.json"
			}
		],
		"commands": [
			{
				"command": "tyk-schemas.main",
				"title": "Tyk Schemas"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"unit": "jest ./src/tests"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.66.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"jest": "^28.1.1",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5"
	}
}
